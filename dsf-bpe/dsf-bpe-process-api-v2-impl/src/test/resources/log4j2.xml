<Configuration status="WARN" monitorInterval="30"
			   verbose="false">

	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="%p\t%t - %C{1}.%M(%L) | %m%n"/>
		</Console>
	</Appenders>

	<Loggers>
		<Logger name="dev.dsf" level="TRACE"/>

		<Root level="WARN">
			<AppenderRef ref="CONSOLE"/>
		</Root>
	</Loggers>
</Configuration>