		</element>
		<element id="Organization.endpoint">
			<path value="Organization.endpoint"/>
			<min value="1"/>
			<type>
				<code value="Reference"/>
				<targetProfile value="http://dsf.dev/fhir/StructureDefinition/endpoint"/>
			</type>
		</element>
	</differential>
